<%= form_for @user do |f| %>
    <%= f.label :name%>
    <%= f.text_field :name %><br/>
    <%= f.label :username%>
    <%= f.text_field :username %><br/>
    <%= f.label :password %>
    <%= f.password_field :password %><br/>
    <%= f.label :bio %>
    <%= f.text_area :bio %><br/>

    <%= f.submit "Submit!"%>
<% end %>